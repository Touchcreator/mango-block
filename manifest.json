{
    "manifest_version": 3,
    "name": "Mango Block",
    "version": "0.6.7",
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.discord.com/*", "*://*.youtube.com/*"],
            "js": ["data/script.js"]
        }
    ],
    "web_accessible_resources": [
        {

            "resources": ["data/block.html"],
            "matches": ["*://*.discord.com/*", "*://*.youtube.com/*"]

        }
    ],
    "action": {
        "default_popup": "data/popup.html"
    }
}